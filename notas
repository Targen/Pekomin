abiertos = []
cerrados = []
abiertos.add camino(nodos = [inicial], costo = 0)
while (abiertos is not empty) {
        c = selecciona un camino abierto
        cerrados.add c
        for (todos los sucesores del nodo terminal del camino c) {
                abiertos.add camino(nodos = c.nodos ++ [n], costo = c.costo + peso(arista(c.nodos.last, n)))
        }
        itera en los caminos cerrados; 
                si encuentras varios que terminan en el mismo nodo, elimina todos menos el de m√≠nimo costo de ellos.
}
