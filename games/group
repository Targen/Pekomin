player {
        class = Player;
        pos.x = 0     ;
        pos.y = 0     ;
        pos.z = 0     ;
        ang   = 0     ;
        vel.x = 0     ;
        vel.y = 0     ;
        vel.z = 0     ;
        vrot  = 0     ;
}

doc {
        pos.x = 10; pos.y = 1;
        LookWhereYoureGoing { class = LookWhereYoureGoing; target = player ; maxAngularVelocity = 0.001; targetRadius = 0; slowRadius = 1; }
        Arrive              { class = Arrive             ; target = player ; maxSpeed = 0.02; targetRadius = 5; slowRadius = 10; }
        Flee_player         { class = Flee               ; target = player ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_grumpy         { class = Flee               ; target = grumpy ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_happy          { class = Flee               ; target = happy  ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_sleepy         { class = Flee               ; target = sleepy ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_bashful        { class = Flee               ; target = bashful; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_sneezy         { class = Flee               ; target = sneezy ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_dopey          { class = Flee               ; target = dopey  ; maxSpeed = 0.01; fleeRadius = 3; }
}

