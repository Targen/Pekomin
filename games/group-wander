player {
        class = Player;
        pos.x = 0     ;
        pos.y = 0     ;
        pos.z = 0     ;
        ang   = 0     ;
        vel.x = 0     ;
        vel.y = 0     ;
        vel.z = 0     ;
        vrot  = 0     ;
}

Wanderer {
        class = RuntimePekomin;
        pos.x = 0;
        pos.y = 0;
        pos.z = 0;
        ang   = 0;
        vel.x = 0;
        vel.y = 0;
        vel.z = 0;
        vrot  = 0;
        Wander { class = Wander; maxAngularAcceleration = 0.01; maxRotation = 0.2; targetRadius = 3; slowRadius = 5; wanderOffset = 1; wanderRadius = 2; wanderRate = 1.2; wanderTime = 3000; maxAcceleration = 0.01; }
}

doc {
        class = RuntimePekomin;
        pos.x = 10; pos.y = 1;
        LookWhereYoureGoing { class = LookWhereYoureGoing; target = Wanderer; maxAngularVelocity = 0.001; targetRadius = 0; slowRadius = 1; }
        Arrive              { class = Arrive             ; target = Wanderer; maxSpeed = 0.02; targetRadius = 5; slowRadius = 10; }
        Flee_Wanderer       { class = Flee               ; target = Wanderer; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_grumpy         { class = Flee               ; target = grumpy  ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_happy          { class = Flee               ; target = happy   ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_sleepy         { class = Flee               ; target = sleepy  ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_bashful        { class = Flee               ; target = bashful ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_sneezy         { class = Flee               ; target = sneezy ; maxSpeed = 0.01; fleeRadius = 3; }
        Flee_dopey          { class = Flee               ; target = dopey  ; maxSpeed = 0.01; fleeRadius = 3; }
}

